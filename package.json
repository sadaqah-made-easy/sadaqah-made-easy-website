{
  "name": "coming-soon-boilerplate",
  "version": "1.0.0",
  "description": "Nextjs and Tailwindcss single page boilerplate",
  "author": "Themefisher",
  "license": "MIT",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "preview": "next start",
    "postbuild": "next-sitemap",
    "build:worker": "cloudflare",
    "dev:worker": "wrangler dev --port 8771",
    "preview:worker": "npm run build:worker && npm run dev:worker",
    "deploy:worker": "npm run build:worker && wrangler deploy",
    "format": "prettier -w ./src"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.7",
    "date-fns": "^4.1.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.0",
    "negotiator": "^1.0.0",
    "next": "14.2.3",
    "next-mdx-remote": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^5.3.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^0.2.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/marked": "^5.0.2",
    "@types/negotiator": "^0.6.3",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "sass": "^1.80.7",
    "sharp": "^0.33.5",
    "tailwind-bootstrap-grid": "^5.1.0",
    "tailwindcss": "^3.4.14",
    "typescript": "5.6.3"
  }
}
